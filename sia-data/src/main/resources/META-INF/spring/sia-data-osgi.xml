<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"                               
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"                
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd                     
       	http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">
    
    <osgi:reference id="hibernateConfigurationRef" interface="com.sia.main.plugin.persistence.HibernateConfiguration" availability="mandatory" timeout="5000" >
    	<osgi:reference-listener>
    		<bean class="com.sia.main.data.sessionfactory.HibernateConfigReferenceListener"></bean>
    	</osgi:reference-listener>
    </osgi:reference>
    
    
    <osgi:service id="basicDAOServ" interface="com.sia.main.data.dao.BasicDAO" ref="basicDAO"/>
        
    <osgi:service id="menuDAOServ" interface="com.sia.main.data.dao.MenuDAO" ref="menuDAO"/>
    
    <osgi:service id="menuPeranDAOServ" interface="com.sia.main.data.dao.MenuPeranDAO" ref="menuPeranDAO"/>
    
    <osgi:service id="modulDAOServ" interface="com.sia.main.data.dao.ModulDAO" ref="modulDAO"/>
    
    <osgi:service id="pdDAOServ" interface="com.sia.main.data.dao.PdDAO" ref="pdDAO"/>
    
    <osgi:service id="penggunaDAOServ" interface="com.sia.main.data.dao.PenggunaDAO" ref="penggunaDAO"/>
    
    <osgi:service id="peranDAOServ" interface="com.sia.main.data.dao.PeranDAO" ref="peranDAO"/>
    
    <osgi:service id="peranPenggunaDAOServ" interface="com.sia.main.data.dao.PeranPenggunaDAO" ref="peranPenggunaDAO"/>
    
    <osgi:service id="ptkDAOServ" interface="com.sia.main.data.dao.PtkDAO" ref="ptkDAO"/>
    
    <osgi:service id="satuanManajemenDAOServ" interface="com.sia.main.data.dao.SatuanManajemenDAO" ref="satuanManajemenDAO"/>
    
    <osgi:service id="statusPluginDAOServ" interface="com.sia.main.data.dao.StatusPluginDAO" ref="statusPluginDAO"/>
    
   <osgi:service id="tipePenggunaDAOServ" interface="com.sia.main.data.dao.TipePenggunaDAO" ref="tipePenggunaDAO"/> 
    
</beans>
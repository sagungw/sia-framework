<?xml version="1.0" encoding="UTF-8"?>
<ipojo 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="org.apache.felix.ipojo http://felix.apache.org/ipojo/schemas/CURRENT/core.xsd" 
    xmlns="org.apache.felix.ipojo">

  	<component name="menuPeranService" classname="com.sia.main.service.services.impl.MenuPeranServiceImpl">
    	<provides specifications="com.sia.main.service.services.MenuPeranService"/>
    	<requires field="menuPeranRepository"></requires>
  	</component>

	<component name="menuService" classname="com.sia.main.service.services.impl.MenuServiceImpl">
    	<provides specifications="com.sia.main.service.services.MenuService"/>
    	<requires field="menuRepository"></requires>
  	</component>
  	
  	<component name="modulService" classname="com.sia.main.service.services.impl.ModulServiceImpl">
    	<provides specifications="com.sia.main.service.services.ModulService"/>
    	<requires field="modulRepository"></requires>
  	</component>

  	<component name="penggunaService" classname="com.sia.main.service.services.impl.PenggunaServiceImpl">
    	<provides specifications="com.sia.main.service.services.PenggunaService"/>
    	<requires field="penggunaRepository"></requires>
  	</component>
  
	<component name="peranPenggunaService" classname="com.sia.main.service.services.impl.PeranPenggunaServiceImpl">
    	<provides specifications="com.sia.main.service.services.PeranPenggunaService"/>
    	<requires field="peranPenggunaRepository"></requires>
  	</component>
  	
	<component name="peranService" classname="com.sia.main.service.services.impl.PeranServiceImpl">
    	<provides specifications="com.sia.main.service.services.PeranService"/>
    	<requires field="peranRepository"></requires>
  	</component>

	<component name="satuanManajemenService" classname="com.sia.main.service.services.impl.SatuanManajemenServiceImpl">
    	<provides specifications="com.sia.main.service.services.SatuanManajemenService"/>
    	<requires field="satuanManajemenRepository"></requires>
  	</component>

  	<instance component="menuPeranService"/>
  	
  	<instance component="menuService"/>
  	
  	<instance component="modulService"/>
  	
  	<instance component="penggunaService"/>
  	
  	<instance component="peranPenggunaService"/>
  	
  	<instance component="peranService"/>
  	
  	<instance component="satuanManajemenService"/>

	<component name="dummyService" classname="com.sia.main.service.services.impl.DummyServiceImpl">
    	<provides specifications="com.sia.main.service.services.DummyService"/>
  	</component>
  	
  	<instance component="dummyService"/>

</ipojo>
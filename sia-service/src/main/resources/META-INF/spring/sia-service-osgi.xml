	<osgi:reference id="MenuPeranRepoRef" interface="com.sia.main.data.repositories.MenuPeranRepository" />
    
    <osgi:reference id="MenuRepoRef" interface="com.sia.main.data.repositories.MenuRepository" />
    
    <osgi:reference id="ModulRepoRef" interface="com.sia.main.data.repositories.ModulRepository" />
    
    <osgi:reference id="PenggunaRepoRef" interface="com.sia.main.data.repositories.PenggunaRepository" />
    
    <osgi:reference id="PeranPenggunaRepoRef" interface="com.sia.main.data.repositories.PeranPenggunaRepository" />
    
    <osgi:reference id="PeranRepoRef" interface="com.sia.main.data.repositories.PeranRepository" />
    
    <osgi:reference id="SatuanManajemenRepoRef" interface="com.sia.main.data.repositories.SatuanManajemenRepository" />

</beans>
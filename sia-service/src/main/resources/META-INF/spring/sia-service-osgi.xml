<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"                               
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"                
   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd                     
       	http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

	<osgi:service id="menuPeranServiceServ" interface="com.sia.main.service.services.MenuPeranService" ref="menuPeranServiceBean"/>
    
    <osgi:service id="menuServiceServ" interface="com.sia.main.service.services.MenuService" ref="menuServiceBean"/>
    
    <osgi:service id="modulServiceServ" interface="com.sia.main.service.services.ModulService" ref="modulServiceBean"/>
    
    <osgi:service id="penggunaServiceServ" interface="com.sia.main.service.services.PenggunaService" ref="penggunaServiceBean"/>
    
    <osgi:service id="peranPenggunaServiceServ" interface="com.sia.main.service.services.PeranPenggunaService" ref="peranPenggunaServiceBean"/>
    
    <osgi:service id="peranServiceServ" interface="com.sia.main.service.services.PeranService" ref="peranServiceBean"/>
    
    <osgi:service id="satuanManajemenServiceServ" interface="com.sia.main.service.services.SatuanManajemenService" ref="satuanManajemenServiceBean"/>
    
    <osgi:service id="statusPluginServiceServ" interface="com.sia.main.service.services.StatusPluginService" ref="statusPluginServiceBean"/>
    
    <osgi:service id="testServiceServ" interface="com.sia.main.service.services.GenericService" ref="testServiceBean"/>


	<osgi:reference id="menuPeranRepoRef" interface="com.sia.main.data.repositories.MenuPeranRepository" availability="mandatory"/>
    
    <osgi:reference id="menuRepoRef" interface="com.sia.main.data.repositories.MenuRepository" availability="mandatory"/>
    
    <osgi:reference id="modulRepoRef" interface="com.sia.main.data.repositories.ModulRepository" availability="mandatory"/>
    
    <osgi:reference id="penggunaRepoRef" interface="com.sia.main.data.repositories.PenggunaRepository" availability="mandatory"/>
    
    <osgi:reference id="peranPenggunaRepoRef" interface="com.sia.main.data.repositories.PeranPenggunaRepository" availability="mandatory"/>
    
    <osgi:reference id="peranRepoRef" interface="com.sia.main.data.repositories.PeranRepository" availability="mandatory"/>
    
    <osgi:reference id="satuanManajemenRepoRef" interface="com.sia.main.data.repositories.SatuanManajemenRepository" availability="mandatory"/>
    
    <osgi:reference id="statusPluginRepoRef" interface="com.sia.main.data.repositories.StatusPluginRepository" availability="mandatory"/>
	
</beans>